<h2 class="h2_search">Search Results</h2>
<div class="cards-container">
  <div class="row">
    <% @trips.each do |trip| %>
      <div class="col-xs-12 col-md-6 col-md-6">
          <%= link_to trip_path(trip, query: @params) do %>
            <h5><%= trip.destination %></h5>
          </div>
          <% end %>
      </div>
  <% end %>
  </div>
</div>
